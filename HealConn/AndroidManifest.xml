<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healconn"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.healconn.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.healconn.ScheduleActivity"
            android:label="@string/title_activity_schedule" >
        </activity>
        <activity
            android:name="com.example.healconn.NewsActivity"
            android:label="@string/title_activity_news" >
        </activity>
        <activity
            android:name="com.example.healconn.FormsActivity"
            android:label="@string/title_activity_forms" >
        </activity>
        <activity
            android:name="com.example.healconn.FeedbackFormActivity"
            android:label="@string/title_activity_feedback_form" >
        </activity>
        <activity
            android:name="com.example.healconn.MessengerActivity"
            android:label="@string/title_activity_messenger" >
        </activity>
        <activity
            android:name="com.example.healconn.NewsWebViewActivity"
            android:label="@string/title_activity_news_web_view" >
        </activity>
    </application>

</manifest>
